U+00
; 00 NULL (FORMAT)
= NUL
* This is a meaningless format character which can be used for byte-padding when encoding texts
> 16 IDLE
> 7F NOTHING

U+01
; 01 HEAD (TRANSMISSION)
= START OF HEADING
= SOH

U+02
; 02 BEGIN (TRANSMISSION)
= START OF TEXT
= STX

U+03
; 03 FINISH (TRANSMISSION)
= END OF TEXT
= ETX

U+04
; 04 DONE (TRANSMISSION)
= END OF TRANSMISSION
= EOT

U+05
; 05 ENQUIRY (CONTROL)
= ENQ
> 06 CONFIRM
> 15 ERROR

U+06
; 06 CONFIRM (CONTROL)
= ACKNOWLEDGE
= ACK
> 05 ENQUIRY
> 15 ERROR

U+07
; 07 ALERT (CONTROL)
= BELL
= BEL

U+08
; 08 BACK (CONTROL)
= BACKSPACE
= BS
* This character is for manipulation of the input position

U+09
; 09 NEXT (CONTROL)
= CHARACTER TABULATION
= HORIZONTAL TABULATION
= HT
= TAB
* This character is for manipulation of the input position
* Using this character to represent indentation is not advised

U+0A
; 0A ADVANCE (CONTROL)
= LINE FEED
= NEW LINE
= END OF LINE
= LF
= NL
* This character is for manipulation of the input position

U+0B
; 0B LINE NEXT (CONTROL)
= LINE TABULATION
= VERTICAL TABULATION
= VT

U+0C
; 0C PAGE NEXT (CONTROL)
= FORM FEED
= FF

U+0D
; 0D LINE START (CONTROL)
= CARRIAGE RETURN
= CR
* This character is for manipulation of the input position

U+0E
; 0E LEAVE (DATA)
= SHIFT OUT
= LOCKING-SHIFT ONE
= SO
* Not suitable for text contents
> 0F RETURN

U+0F
; 0F RETURN (DATA)
= SHIFT IN
= LOCKING-SHIFT ZERO
= SI
* Not suitable for text contents
> 0E LEAVE

U+10
; 10 DATA (DATA)
= DATA LINK ESCAPE
= DLE

U+11
; 11 BOOT (CONTROL)
= DEVICE CONTROL ONE
= DC1

U+12
; 12 BOOT ALTERNATE (CONTROL)
= DEVICE CONTROL TWO
= DC2

U+13
; 13 WAIT (CONTROL)
= DEVICE CONTROL THREE
= DC3

U+14
; 14 HALT (CONTROL)
= DEVICE CONTROL FOUR
= DC4

U+15
; 15 ERROR (CONTROL)
= NEGATIVE ACKNOWLEDGE
= NAK
> 05 ENQUIRY
> 06 CONFIRM

U+16
; 16 IDLE (TRANSMISSION)
= SYNCHRONOUS IDLE
= SYN
* This is a meaningless transmission character for padding a stream
* Use 00 for padding characters in storage
> 00 NULL
> 7F NOTHING

U+17
; 17 BREAK (TRANSMISSION)
= END OF TRANSMISSION BLOCK
= ETB

U+18
; 18 CANCEL (TRANSMISSION)
= CAN

U+19
; 19 END (TRANSMISSION)
= END OF MEDIUM
= EOM

U+1A
; 1A INVALID (TRANSMISSION)
= SUBSTITUTE
= SUB
* Indicates a corrupted or invalid character; for use in transmission only

U+1B
; 1B COMMAND (CONTROL)
= ESCAPE
= ESC

U+1C
